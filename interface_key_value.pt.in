PTVERSION           = 2
NAME                = @PACKAGE@
VERSION             = @PACKAGE_VERSION@
DESCRIPTION         = robotkernel @PACKAGE@
MAINTAINER          = Robert Burger <robert.burger@dlr.de>
DOCUMENTATION       = https://rmintra01.robotic.dlr.de/wiki/Robotkernel/interface_key_value
ARCHS               = @ARCHS@
SUPPORTED_LANGUAGES = C
C_INCLUDE_PATHS     = ${PKGROOT}/include/
C_LIB_PATHS         = ${PKGROOT}/lib/${ARCH}
C_LIBS              = key_value_helper

[install]
build/${ARCH}/install/lib => lib/${ARCH}
build/sled11-x86-gcc4.x/install/include/@PACKAGE@ => include/@PACKAGE@
doc/ => doc/
share/ => share/
